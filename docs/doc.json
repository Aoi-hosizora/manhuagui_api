  "tags": [
    {
      "name": "Manga",
      "description": "manga-controller"
    }
  ],
  "paths": {
    "/v1/manga/{mid}": {
      "get": {
        "summary": "Get manga page",
        "operationId": "-v1-manga-mid-get",
        "tags": [
          "Manga"
        ],
        "parameters": [
          {
            "name": "mid",
            "in": "path",
            "required": true,
            "type": "integer",
            "description": "manga id",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "originRef": "_Result<MangaPageDto>",
              "$ref": "#/definitions/_Result<MangaPageDto>"
            }
          }
        }
      }
    },
    "/v1/manga/{mid}/{cid}": {
      "get": {
        "summary": "Get manga chapter",
        "operationId": "-v1-manga-mid-cid-get",
        "tags": [
          "Manga"
        ],
        "parameters": [
          {
            "name": "mid",
            "in": "path",
            "required": true,
            "type": "integer",
            "description": "manga id",
            "format": "int64"
          },
          {
            "name": "cid",
            "in": "path",
            "required": true,
            "type": "integer",
            "description": "manga chapter id",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "originRef": "_Result<MangaChapterDto>",
              "$ref": "#/definitions/_Result<MangaChapterDto>"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "MangaChapterDto": {
      "type": "object",
      "required": [
        "mid",
        "mname",
        "cid",
        "cname",
        "pages",
        "next_cid",
        "prev_cid"
      ],
      "description": "Mange chapter response",
      "properties": {
        "mid": {
          "type": "integer",
          "required": true,
          "description": "manga id",
          "format": "int64"
        },
        "mname": {
          "type": "string",
          "required": true,
          "description": "manga name"
        },
        "cid": {
          "type": "integer",
          "required": true,
          "description": "manga chapter id",
          "format": "int64"
        },
        "cname": {
          "type": "string",
          "required": true,
          "description": "manga chapter name"
        },
        "pages": {
          "type": "array",
          "required": true,
          "description": "manga pages",
          "items": {
            "type": "string"
          }
        },
        "next_cid": {
          "type": "integer",
          "required": true,
          "description": "manga next chapter id",
          "format": "int64"
        },
        "prev_cid": {
          "type": "integer",
          "required": true,
          "description": "manga prev chapter id",
          "format": "int64"
        }
      }
    },
    "MangaPageDto": {
      "type": "object",
      "required": [
        "mid"
      ],
      "description": "Mange page response",
      "properties": {
        "mid": {
          "type": "integer",
          "required": true,
          "description": "manga id",
          "format": "int64"
        }
      }
    },
    "Result": {
      "type": "object",
      "required": [
        "code",
        "message"
      ],
      "description": "global response",
      "properties": {
        "code": {
          "type": "integer",
          "required": true,
          "description": "status code",
          "format": "int32"
        },
        "message": {
          "type": "string",
          "required": true,
          "description": "status message"
        }
      }
    },
    "_Result<MangaChapterDto>": {
      "type": "object",
