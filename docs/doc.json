{
  "swagger": "2.0",
  "host": "localhost:10018",
  "basePath": "/",
  "info": {
    "title": "manhuagui-backend",
    "description": "An unofficial backend for manhuagui written in golang/gin",
    "version": "1.0",
    "contact": {
      "name": "Aoi-hosizora",
      "url": "https://github.com/Aoi-hosizora",
      "email": "aoihosizora@hotmail.com"
    }
  },
  "tags": [
    {
      "name": "Manga",
      "description": "manga-controller"
    }
  ],
  "paths": {
    "/v1/list/finish": {
      "get": {
        "summary": "Get finished mangas",
        "operationId": "-v1-list-finish-get",
        "tags": [
          "Manga"
        ],
        "responses": {
          "200": {
            "schema": {
              "originRef": "_Result<MangaGroupListDto>",
              "$ref": "#/definitions/_Result<MangaGroupListDto>"
            }
          }
        }
      }
    },
    "/v1/list/latest": {
      "get": {
        "summary": "Get latest mangas",
        "operationId": "-v1-list-latest-get",
        "tags": [
          "Manga"
        ],
        "responses": {
          "200": {
            "schema": {
              "originRef": "_Result<MangaGroupListDto>",
              "$ref": "#/definitions/_Result<MangaGroupListDto>"
            }
          }
        }
      }
    },
    "/v1/list/serial": {
      "get": {
        "summary": "Get hot serial mangas",
        "operationId": "-v1-list-serial-get",
        "tags": [
          "Manga"
        ],
        "responses": {
          "200": {
            "schema": {
              "originRef": "_Result<MangaGroupListDto>",
              "$ref": "#/definitions/_Result<MangaGroupListDto>"
            }
          }
        }
      }
    },
    "/v1/manga/{mid}": {
      "get": {
        "summary": "Get manga page",
        "operationId": "-v1-manga-mid-get",
        "tags": [
          "Manga"
        ],
        "parameters": [
          {
            "name": "mid",
            "in": "path",
            "required": true,
            "type": "integer",
            "description": "manga id",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "originRef": "_Result<MangaPageDto>",
              "$ref": "#/definitions/_Result<MangaPageDto>"
            }
          }
        }
      }
    },
    "/v1/manga/{mid}/{cid}": {
      "get": {
        "summary": "Get manga chapter",
        "operationId": "-v1-manga-mid-cid-get",
        "tags": [
          "Manga"
        ],
        "parameters": [
          {
            "name": "mid",
            "in": "path",
            "required": true,
            "type": "integer",
            "description": "manga id",
            "format": "int64"
          },
          {
            "name": "cid",
            "in": "path",
            "required": true,
            "type": "integer",
            "description": "manga chapter id",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "schema": {
              "originRef": "_Result<MangaChapterDto>",
              "$ref": "#/definitions/_Result<MangaChapterDto>"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "MangaChapterDto": {
      "type": "object",
      "required": [
        "mid",
        "mname",
        "cid",
        "cname",
        "url",
        "pages",
        "next_cid",
        "prev_cid"
      ],
      "description": "Mange chapter response",
      "properties": {
        "mid": {
          "type": "integer",
          "required": true,
          "description": "manga id",
          "format": "int64"
        },
        "mname": {
          "type": "string",
          "required": true,
          "description": "manga name"
        },
        "cid": {
          "type": "integer",
          "required": true,
          "description": "manga chapter id",
          "format": "int64"
        },
        "cname": {
          "type": "string",
          "required": true,
          "description": "manga chapter name"
        },
        "url": {
          "type": "string",
          "required": true,
          "description": "manga chapter link"
        },
        "pages": {
          "type": "array",
          "required": true,
          "description": "manga chapter pages",
          "items": {
            "type": "string"
          }
        },
        "next_cid": {
          "type": "integer",
          "required": true,
          "description": "manga next chapter id",
          "format": "int64"
        },
        "prev_cid": {
          "type": "integer",
          "required": true,
          "description": "manga prev chapter id",
          "format": "int64"
        }
      }
    },
    "MangaChapterGroupDto": {
      "type": "object",
      "required": [
        "title",
        "chapters"
      ],
      "description": "Mange chapter group response",
      "properties": {
        "title": {
          "type": "string",
          "required": true,
          "description": "chapter group title"
        },
        "chapters": {
          "type": "array",
          "required": true,
          "description": "chapter group chapters",
          "items": {
            "originRef": "MangaChapterLinkDto",
            "$ref": "#/definitions/MangaChapterLinkDto"
          }
        }
      }
    },
    "MangaChapterLinkDto": {
      "type": "object",
      "required": [
        "cid",
        "cname",
        "page_count",
        "url",
        "new"
      ],
      "description": "Manga chapter link response",
      "properties": {
        "cid": {
          "type": "integer",
          "required": true,
          "description": "manga chapter id",
          "format": "int64"
        },
        "cname": {
          "type": "string",
          "required": true,
          "description": "manga chapter name"
        },
        "page_count": {
          "type": "integer",
          "required": true,
          "description": "manga chapter page number",
          "format": "int32"
        },
        "url": {
          "type": "string",
          "required": true,
          "description": "manga chapter link"
        },
        "new": {
          "type": "boolean",
          "required": true,
          "description": "manga chapter is uploaded newly"
        }
      }
    },
    "MangaGroupListDto": {
      "type": "object",
      "required": [
        "title",
        "top_group",
        "groups",
        "other_groups"
      ],
      "description": "Manga group list",
      "properties": {
        "title": {
          "type": "string",
          "required": true,
          "description": "manga group title"
        },
        "top_group": {
          "required": true,
          "description": "manga top page group",
          "originRef": "MangaPageGroupDto",
          "$ref": "#/definitions/MangaPageGroupDto"
        },
        "groups": {
          "type": "array",
          "required": true,
          "description": "manga page groups",
          "items": {
            "originRef": "MangaPageGroupDto",
            "$ref": "#/definitions/MangaPageGroupDto"
          }
        },
        "other_groups": {
          "type": "array",
          "required": true,
          "description": "manga other page groups",
          "items": {
            "originRef": "MangaPageGroupDto",
            "$ref": "#/definitions/MangaPageGroupDto"
          }
        }
      }
    },
    "MangaPageDto": {
      "type": "object",
      "required": [
        "mid",
        "mname",
        "cover",
        "url",
        "publish_year",
        "alphabet_index",
        "type",
        "author_name",
        "alias",
        "finished",
        "newest_chapter",
        "newest_date",
        "introduction",
        "rank",
        "average_score",
        "score_count",
        "per_scores",
        "groups"
      ],
      "description": "Mange page response",
      "properties": {
        "mid": {
          "type": "integer",
          "required": true,
          "description": "manga id",
          "format": "int64"
        },
        "mname": {
          "type": "string",
          "required": true,
          "description": "manga name"
        },
        "cover": {
          "type": "string",
          "required": true,
          "description": "manga cover"
        },
        "url": {
          "type": "string",
          "required": true,
          "description": "manga link"
        },
        "publish_year": {
          "type": "string",
          "required": true,
          "description": "manga publish year"
        },
        "alphabet_index": {
          "type": "string",
          "required": true,
          "description": "manga alphabet index"
        },
        "type": {
          "type": "string",
          "required": true,
          "description": "manga type"
        },
        "author_name": {
          "type": "string",
          "required": true,
          "description": "manga author name"
        },
        "alias": {
          "type": "string",
          "required": true,
          "description": "manga alias name"
        },
        "finished": {
          "type": "boolean",
          "required": true,
          "description": "manga is finished"
        },
        "newest_chapter": {
          "type": "string",
          "required": true,
          "description": "manga last update chapter"
        },
        "newest_date": {
          "type": "string",
          "required": true,
          "description": "manga last update date"
        },
        "introduction": {
          "type": "string",
          "required": true,
          "description": "manga introduction"
        },
        "rank": {
          "type": "string",
          "required": true,
          "description": "manga rank"
        },
        "average_score": {
          "type": "number",
          "required": true,
          "description": "manga average score",
          "format": "float"
        },
        "score_count": {
          "type": "integer",
          "required": true,
          "description": "manga score count",
          "format": "int32"
        },
        "per_scores": {
          "type": "array",
          "required": true,
          "description": "manga per scores, from 0 to 5",
          "items": {
            "type": "number",
            "format": "float"
          }
        },
        "groups": {
          "type": "array",
          "required": true,
          "description": "manga chapter groups",
          "items": {
            "originRef": "MangaChapterGroupDto",
            "$ref": "#/definitions/MangaChapterGroupDto"
          }
        }
      }
    },
    "MangaPageGroupDto": {
      "type": "object",
      "required": [
        "title",
        "mangas"
      ],
      "description": "Mange page group response",
      "properties": {
        "title": {
          "type": "string",
          "required": true,
          "description": "manga group title"
        },
        "mangas": {
          "type": "array",
          "required": true,
          "description": "manga group mangas",
          "items": {
            "originRef": "MangaChapterLinkDto",
            "$ref": "#/definitions/MangaChapterLinkDto"
          }
        }
      }
    },
    "MangaPageLinkDto": {
      "type": "object",
      "required": [
        "mid",
        "mname",
        "cover",
        "url",
        "finished",
        "newest_chapter"
      ],
      "description": "Manga page link response",
      "properties": {
        "mid": {
          "type": "integer",
          "required": true,
          "description": "manga id",
          "format": "int64"
        },
        "mname": {
          "type": "string",
          "required": true,
          "description": "manga name"
        },
        "cover": {
          "type": "string",
          "required": true,
          "description": "manga cover"
        },
        "url": {
          "type": "string",
          "required": true,
          "description": "manga link"
        },
        "finished": {
          "type": "boolean",
          "required": true,
          "description": "manga is finished"
        },
        "newest_chapter": {
          "type": "string",
          "required": true,
          "description": "manga last update chapter"
        }
      }
    },
    "Result": {
      "type": "object",
      "required": [
        "code",
        "message"
      ],
      "description": "global response",
      "properties": {
        "code": {
          "type": "integer",
          "required": true,
          "description": "status code",
          "format": "int32"
        },
        "message": {
          "type": "string",
          "required": true,
          "description": "status message"
        }
      }
    },
    "_Result<MangaChapterDto>": {
      "type": "object",
      "required": [
        "code",
        "message",
        "data"
      ],
      "description": "global response",
      "properties": {
        "code": {
          "type": "integer",
          "required": true,
          "description": "status code",
          "format": "int32"
        },
        "message": {
          "type": "string",
          "required": true,
          "description": "status message"
        },
        "data": {
          "required": true,
          "description": "response data",
          "originRef": "MangaChapterDto",
          "$ref": "#/definitions/MangaChapterDto"
        }
      }
    },
    "_Result<MangaGroupListDto>": {
      "type": "object",
      "required": [
        "code",
        "message",
        "data"
      ],
      "description": "global response",
      "properties": {
        "code": {
          "type": "integer",
          "required": true,
          "description": "status code",
          "format": "int32"
        },
        "message": {
          "type": "string",
          "required": true,
          "description": "status message"
        },
        "data": {
          "required": true,
          "description": "response data",
          "originRef": "MangaGroupListDto",
          "$ref": "#/definitions/MangaGroupListDto"
        }
      }
    },
    "_Result<MangaPageDto>": {
      "type": "object",
      "required": [
        "code",
        "message",
        "data"
      ],
      "description": "global response",
      "properties": {
        "code": {
          "type": "integer",
          "required": true,
          "description": "status code",
          "format": "int32"
        },
        "message": {
          "type": "string",
          "required": true,
          "description": "status message"
        },
        "data": {
          "required": true,
          "description": "response data",
          "originRef": "MangaPageDto",
          "$ref": "#/definitions/MangaPageDto"
        }
      }
    }
  }
}
